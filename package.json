{
  "name": "korbots",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run client-build && npm run server-start",
    "build": "npm run client-build && npm run server-build",
    "clean": "npm run client-clean && npm run server-clean",
    "doc": "npm run server-doc",
    "install": "npm run client-install && npm run server-install",

    "client-install": "npm install --prefix=./client && npm install --save-dev --prefix=./client",
    "client-build": "npm run build --prefix=./client",
    "client-clean": "npm run clean --prefix=./client",
    "server-install": "npm install --prefix=./server && npm install --save-dev --prefix=./server",
    "server-build": "npm run build --prefix=./server",
    "server-clean": "npm run clean --prefix=./server",
    "server-start": "npm run start --prefix=./server",
    "server-doc": "npm run doc --prefix=./server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xisabla/Korbots.git"
  },
  "author": "Xisabla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xisabla/Korbots/issues"
  },
  "homepage": "https://github.com/Xisabla/Korbots#readme"
}
