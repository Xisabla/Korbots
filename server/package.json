{
  "name": "korbots-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env DEBUG=core:Application node dist/index.js",
    "dev": "npm run prestart && cross-env DEBUG=core:*,model:*,module:*,task:*,service:* node dist/index.js",
    "build": "tsc --project ./tsconfig.json",
    "clean": "rimraf ./coverage ./dist ../doc/server",
    "lint": "eslint ./ --fix",
    "doc": "typedoc --out ../doc/server --plugin typedoc-plugin-markdown --readme none",
    "postinstall": "patch-package",
    "prestart": "npm run lint && npm run build"
  },
  "author": "Xisabla",
  "license": "MIT",
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.7.32",
    "@types/node-fetch": "^2.5.7",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-client": "^1.4.33",
    "@types/youtube-dl": "^1.12.2",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "socket.io-client": "^2.3.0",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.3.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.2.6",
    "moment": "^2.27.0",
    "mongoose": "^5.9.25",
    "node-fetch": "^2.6.1",
    "popyt": "^4.1.0",
    "socket.io": "^2.3.0",
    "youtube-dl": "^3.0.2"
  }
}
